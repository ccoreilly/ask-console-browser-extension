{
  "manifest_version": 2,
  "name": "Alexa Skills Kit Simulator Enhancer",
  "description": "Speed up Alexa Skill testing in the Alexa Skill Console",
  "version": "1.1",
  "icons": {
    "48": "/img/icon_48.png",
    "128": "/img/icon_128.png"
  },
  "content_scripts": [{
    "matches":    ["https://developer.amazon.com/alexa/console/ask/test/*"],
    "js":         ["jquery-3.2.1.min.js", "content.js"],
    "css" : ["style.css"]
  }],
  "permissions": ["storage"],
  "web_accessible_resources" : ["script"]
}
